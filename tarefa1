import pandas as pd
import numpy as np

op = int(input('Digite a rodada desejada: '))

selecao = ['atletas.rodada_id', 'atletas.clube_id', 'FS', 'RB', 'PE', 'FC', 'G', 'FF', 'FT', 'FD', 'DD', 'GS', 'SG', 'A',
           'CA', 'I', 'CV', 'PP', 'GC', 'DP']

if op == 1:
    rodada1 = pd.read_csv('D:/Aender/caRtola-master/data/2019/rodada-1.csv')
    df = rodada1.filter(items=selecao)
    agrupar = df.groupby(['atletas.clube_id', 'atletas.rodada_id'])

    print(agrupar['FS', 'RB', 'PE', 'FC', 'G', 'FF', 'FT', 'FD', 'DD', 'GS', 'SG', 'A', 'CA', 'I', 'CV', 'PP', 'GC',
                  'DP'].agg(np.sum))
elif op == 2:
    rodada2 = pd.read_csv('D:/Aender/caRtola-master/data/2019/rodada-2.csv')
